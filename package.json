{
  "name": "nanohashcode",
  "version": "1.0.0",
  "description": "A tiny (75 bytes), string hash code implementation for JavaScript/TypeScript",
  "keywords": [
    "hash code",
    "javascript",
    "typescript"
  ],
  "scripts": {
    "clean": "rm -R coverage",
    "benchmark": "node test/benchmark.js",
    "unit": "uvu . .test.js$",
    "test": "c8 npm run unit && npm run clean && size-limit",
    "build": "terser index.browser.js -c -m -o index.js"
  },
  "type": "module",
  "engines": {
    "node": "^14 || ^16 || >=18"
  },
  "author": "Aliaksandr Pyrkh <alexpyrkh@gmail.com>",
  "license": "MIT",
  "repository": "apyrkh/nanohashcode",
  "exports": {
    ".": {
      "browser": "./index.browser.js",
      "default": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "browser": {
    "./index.js": "./index.browser.js"
  },
  "sideEffects": false,
  "types": "./index.d.ts",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "benchmark": "^2.1.4",
    "c8": "^7.12.0",
    "picocolors": "^1.0.0",
    "size-limit": "^8.1.0",
    "terser": "^5.16.0",
    "uvu": "^0.5.6"
  }
}
