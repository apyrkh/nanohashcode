{
  "name": "nanohashcode",
  "version": "1.0.0",
  "description": "A tiny (75 bytes), string hash code implementation for JavaScript/TypeScript",
  "keywords": [
    "hash code",
    "javascript",
    "typescript"
  ],
  "scripts": {
    "clean": "rm -R coverage",
    "benchmark": "node test/benchmark.js",
    "unit": "uvu . .test.js$",
    "size-limit": "size-limit",
    "test": "c8 npm run unit && size-limit && npm run clean",
    "build": "terser hashCode.browser.js -c -m -o hashCode.js"
  },
  "author": "Aliaksandr Pyrkh <alexpyrkh@gmail.com>",
  "license": "MIT",
  "repository": "apyrkh/nanohashcode",
  "sideEffects": false,
  "type": "module",
  "types": "./index.d.ts",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^14.0.0 || ^16.0.0 || >=18.0.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "benchmark": "^2.1.4",
    "c8": "^7.12.0",
    "picocolors": "^1.0.0",
    "size-limit": "^8.1.0",
    "terser": "^5.16.0",
    "uvu": "^0.5.6"
  }
}
